
.list {
		list-style: none;
}
.index-item {
		display: flex;
		justify-content: space-between;
		border: 1rpx solid #CDCDCD;
		margin-bottom: 15rpx;
}
.index-item-leftBox {
		padding: 10rpx;
		width: 55%;
}
.index-item-rightBox {
		padding: 10rpx;
		width: 45%;
}
.index-item-leftBox-dayBox-text,.index-item-rightBox-lastDayBox-text,.index-item-rightBox-numBox-text{
		font-size: 14rpx;
}
.index-item-leftBox-titleBox-text{
		font-size: 30rpx;
}
